import java.util.Random;

public class Main {
	
	public static void operacionTrad() {
		Random random = new Random();
        int total = 10; // Definir el tamaño del array
        int[] vec = new int[total];

        for (int i = 0; i < vec.length; i++) {
        	vec[i] = random.nextInt(100); // Genera un número aleatorio entre 0 y 99
        }
        // Imprimir los elementos del array
        System.out.print("Array de 10 elementos aleatorios: ");
        for(int valor : vec) {
            System.out.print(valor + " ");
        }
	}
	
	public static void main(String args[]) {
        MPArray oVector = new MPArray();
		oVector.imprimir();
		operacionTrad();
	}
}
